package br.edu.ifpb.pdp.labirinty;

import java.util.ArrayList;

public class Sala  implements ElementLabirinty{
	private int numberRoom;
	private ElementLabirinty vizinho;
	private ArrayList<ElementLabirinty> elements;
	
	
	public Sala(int number) {
		this.elements = ArrayList<ElementLabirinty>();
		this.numberRoom = number;
	}


	public int getNumberRoom() {
		return numberRoom;
	}


	public ElementLabirinty getVizinho() {
		return vizinho;
	}


	public void setVizinho(ElementLabirinty vizinho) {
		this.vizinho = vizinho;
	}


	@Override
	public void entrar() {
		// TODO Auto-generated method stub
		
	}
	
}
